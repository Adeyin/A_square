	        <ImageButton
		        android:id="@+id/imageButton1"
		        android:layout_width="fill_parent"
		        android:layout_height="fill_parent"
		        android:background="@drawable/ic_launcher"/>
            
		</LinearLayout>
    	<LinearLayout android:id="@+id/linearLayout12" 
			android:layout_height="fill_parent" 
			android:layout_weight="1"
